{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]",
          "iframe[referrerpolicy=no-referrer-when-downgrade]",
          "video[playsinline]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "button-type": "off"
  }
}